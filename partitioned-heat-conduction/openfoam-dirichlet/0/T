FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1;

boundaryField
{
    interface
    {
        type            fixedValue;
        value           $internalField;
    }

    heatSource
    {
        type            fixedValue;
        value           uniform 10;
    }

    left
    {
        type            zeroGradient;
    }

    top
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            empty;
    }
}
